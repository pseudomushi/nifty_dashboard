{
  "buildCommand": "vite build",
  "installCommand": "pnpm install --frozen-lockfile",
  "outputDirectory": "dist/public",
  "rewrites": [
    {
      "source": "/api/trpc/:path*",
      "destination": "/api/trpc"
    },
    {
      "source": "/api/oauth/:path*",
      "destination": "/api/oauth"
    },
    {
      "source": "/((?!api).*)",
      "destination": "/index.html"
    }
  ]
}
